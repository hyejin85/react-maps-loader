{
  "name": "react-maps-loader",
  "version": "independent",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "command": {
    "publish": {
      "conventionalCommits": true
    }
  },
  "scripts": {
    "compile": "rm -rf lib && tsc -p packages/google/tsconfig.json && tsc -p packages/naver/tsconfig.json",
    "build:google": "vite build -c packages/google/vite.config.ts",
    "build:naver": "vite build -c packages/naver/vite.config.ts",
    "build": "npm run build:google && npm run build:naver",
    "clean:dist": "rm -rf packages/google/dist && rm -rf packages/naver/dist",
    "make:dist": "mkdir packages/google/dist && mkdir packages/naver/dist",
    "copy:google": "cp -r dist/common packages/google/dist/common && cp -r dist/google packages/google/dist/google",
    "copy:naver": "cp -r dist/common packages/naver/dist/common && cp -r dist/naver packages/naver/dist/naver",
    "copy:dist": "npm run copy:google && npm run copy:naver",
    "remove": "rm -rf packages/google/dist/naver && rm -rf packages/naver/dist/google",
    "prepare": "npm run clean:dist && npm run build && npm run remove",
    "publish": "lerna publish"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^18.2.56",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "lerna": "^8.1.2",
    "lodash-es": "^4.17.21",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "typescript": "^5.3.3",
    "vite": "^5.2.11",
    "vite-plugin-dts": "^3.9.1"
  }
}
