{
  "name": "react-maps-loader",
  "version": "independent",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "command": {
    "publish": {
      "conventionalCommits": true
    }
  },
  "scripts": {
    "remove": "rm -rf packages/common/build && rm -rf packages/google/build && rm -rf packages/naver/build",
    "copy:babel": "cp babel.config.js packages/common && cp babel.config.js packages/google && cp babel.config.js packages/naver",
    "copy:config": "cp webpack.config.js packages/common && cp webpack.config.js packages/google && cp webpack.config.js packages/naver",
    "copy:build": "cp -r build packages/common && cp -r build packages/google && cp -r build packages/naver",
    "build": "rm -rf lib && npx tsc --build && webpack"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^18.2.56",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-loader": "^9.1.3",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "lerna": "^8.1.2",
    "lodash-es": "^4.17.21",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "clean-webpack-plugin": "^4.0.0",
    "webpack": "^5.90.3",
    "webpack-merge": "^5.10.0"
  }
}
